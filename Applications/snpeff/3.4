#%Module1.0###################################################
#
# This modulefile for package snpeff creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The snpeff module file defines the following environment variables:"
puts stderr "PATH, for "
puts stderr "the location of the snpeff distribution and its libraries.\n"
puts stderr "\nVersion 1.2.8"
}
module-whatis "Name: snpeff"
module-whatis "Version: 3.4"
module-whatis "Category: library, statistical, annotation"
module-whatis "URL: http://snpeff.sourceforge.net/"
module-whatis "Description: bioinformatics annotation software"

prepend-path PATH               /opt/snpeff/3.4/prebuilt
