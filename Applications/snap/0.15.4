#%Module1.0###################################################
#
# This modulefile for package SNAP creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The SNAP module file defines the following environment variables:"
puts stderr "PATH "
puts stderr "the location of the SNAP distribution and its libraries.\n"
puts stderr "\nVersion 0.15.4"
}
module-whatis "Name: SNAP"
module-whatis "Version: 0.15.4"
module-whatis "Category: sequence alignment"
module-whatis "URL: http://snap.cs.berkeley.edu/"
module-whatis "Description: Scalable Nucleotide Alignment Program"

prepend-path PATH               /opt/snap/0.15.4/prebuilt/bin
