#%Module1.0###################################################
#
# This modulefile for package RepeatScout creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The RepeatScout module file defines the following environment variables:"
puts stderr "PATH for "
puts stderr "the location of the RepeatScout distribution and its libraries.\n"
puts stderr "\nVersion 1.0.5"
}
module-whatis "Name: RepeatScout"
module-whatis "Version: 1.0.5"
module-whatis "Category: sequencing, statistical, genomics"
module-whatis "URL: http://repeatscout.bioprojects.org"
module-whatis "Description: Tool for discovering repetitive substrings in DNA"


prepend-path PATH               /opt/repeatscout/1.0.5/icc
