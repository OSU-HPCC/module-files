#%Module1.0###################################################
#
# This modulefile for package makedepend creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The makedepend module file defines the following environment variables:"
puts stderr "PATH for "
puts stderr "the location of the Samtools distribution and its libraries.\n"
puts stderr "\nVersion 1.0.5"
}
module-whatis "Name: makedepend"
module-whatis "Version: 1.0.5"

prepend-path PATH               /opt/makedepend/1.0.5/gcc/bin
