#%Module1.0###################################################
#
# This modulefile for package bedtools creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The bedtools module file defines the following environment variables:"
puts stderr "PATH, for "
puts stderr "the location of the bedtools distribution and its libraries.\n"
puts stderr "\nVersion 2.18.2"
}
module-whatis "Name: bedtools"
module-whatis "Version: 2.18.2"
module-whatis "Category: library"
module-whatis "URL: http://bedtools.readthedocs.org/en/latest/"
module-whatis "Description: genome library"

prepend-path PATH               /opt/bedtools/2.18.2/gcc/bin
