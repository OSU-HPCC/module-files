#
# This modulefile for package REPET creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The REPET module file defines the following environment variables:"
puts stderr "PATH for "
puts stderr "the location of the REPET distribution and its libraries.\n"
puts stderr "\nVersion 1.3.12.1"
}
module-whatis "Name: REPET"
module-whatis "Version: 1.3.12.1"
module-whatis "Category: sequencing, statistical, genomics"
module-whatis "URL: http://urgi.versailles.inra.fr/Tools/REPET"
module-whatis "Description: Integrates bioinformatics programs"

prepend-path PATH		/opt/repet/1.3.12.1/bin
prepend-path REPET_PATH		/opt/repet/1.3.12.1
prepend-path REPET_JOBS		MySQL
prepend-path REPET_QUEUE	PBS
prepend-path PYTHONPATH		/opt/repet/1.3.12.1

module load python/2.7.3
module load wublast/2.0a19
module load blast+/2.2.23
module load recon/1.07
module load piler/1.0
module load censor/4.2.28
module load repeatmasker/4.0.1
module load trf/4.07b
module load mreps/2.5
