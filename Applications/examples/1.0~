#%Module1.0###################################################
#
# This modulefile for package quast  creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The quast module file defines the following environment variables:"
puts stderr "PATH for "
puts stderr "the location of the REPET distribution and its libraries.\n"
puts stderr "\nVersion 2.0"
}
module-whatis "Name: quast"
module-whatis "Version: 2.3"
module-whatis "Category: assembly evaluation "
module-whatis "URL: http://bioinf.spbau.ru/quast"


prepend-path PATH		/opt/quast/2.3
module load anaconda
