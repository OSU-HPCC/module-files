proc ModulesHelp { } {
puts stderr "The rsem module file defines the following environment variables:"
puts stderr "PATH, for "
puts stderr "the location of the rsem distribution and its libraries.\n"
puts stderr "\nVersion 1.2.8"
}
module-whatis "Name: rsem"
module-whatis "Version: 1.2.8"
module-whatis "Category: library, statistical, graphics"
module-whatis "URL: http://deweylab.biostat.wisc.edu/rsem/"
module-whatis "Description: RNA-Seq for high-end reads"

prepend-path PATH               /opt/rsem/1.2.8/gcc

