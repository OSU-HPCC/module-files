proc ModulesHelp { } {
puts stderr "The picard-tools module file defines the following environment variables:"
puts stderr "PATH for "
puts stderr "the location of the picard-tools distribution and its libraries.\n"
puts stderr "\nVersion 1.119"
}
module-whatis "Name: picard-tools"
module-whatis "Version: 1.119"
module-whatis "Category: bioinformatics, genomics, sequencing"
module-whatis "URL: http://picard.sourceforge.net"
module-whatis "Description: "


prepend-path PATH               /opt/picard-tools/1.119
