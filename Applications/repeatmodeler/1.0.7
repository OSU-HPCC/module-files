#%Module1.0###################################################
#
# This modulefile for package RepeatModeler creates a variable so
# that users can easily find the right libraries
#
##############################################################

proc ModulesHelp { } {
puts stderr "The RepeatModeler module file defines the following environment variables:"
puts stderr "PATH for "
puts stderr "the location of the RepeatModeler distribution and its libraries.\n"
puts stderr "\nVersion 1.0.7"
}
module-whatis "Name: RepeatModeler"
module-whatis "Version: 1.0.7"
module-whatis "Category: sequencing, statistical, genomics"
module-whatis "URL: http://www.repeatmasker.org/RepeatModeler.html"
module-whatis "Description: A de-novo repeat family identification and modeling package"


prepend-path PATH               /opt/repeatmodeler/1.0.7/gcc
