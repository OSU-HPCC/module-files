#%Module1.0###################################################
#
# This modulefile for GNU cmake sets the Path variable
# to the bin directory
#
##############################################################

proc ModulesHelp { } {
puts stderr "The GNU cmake modulefile sets the "
puts stderr "PATH variable to"
puts stderr "the location of the cmake libraries.\n"
puts stderr "\nVersion 3.1.3"
}
module-whatis "Name: cmake"
module-whatis "Version: 3.1.3"
module-whatis "Category: compiler"
module-whatis "URL: http://www.cmake.org"

prepend-path PATH	/opt/cmake/3.1.3/gcc-4.7.2/bin
module load gcc-4.7.2
